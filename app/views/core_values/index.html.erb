    <td><%= core_value.teamwork_total %></td>
    <td><%= core_value.gracious_professionalism_total %></td>
    <td><%= core_value.core_value_total %></td>
    <td><%= link_to 'Show', core_value %></td>
    <td><%= link_to 'Edit', edit_core_value_path(core_value) %></td>
    <td><%= link_to 'Destroy', core_value, method: :delete, data: { confirm: 'Are you sure?' } %></td>
