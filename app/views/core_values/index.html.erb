    <th>Core value total</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @core_values.each do |core_value| %>
  <tr>
    <td><%= core_value.team_number %></td>
    <td><%= core_value.discovery %></td>
    <td><%= core_value.team_spirit %></td>
    <td><%= core_value.integration %></td>
    <td><%= core_value.effectiveness %></td>
    <td><%= core_value.efficiency %></td>
    <td><%= core_value.kids_do_the_work %></td>
    <td><%= core_value.inclusion %></td>
    <td><%= core_value.respect %></td>
    <td><%= core_value.coopertition %></td>
